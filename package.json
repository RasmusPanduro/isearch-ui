{
  "name": "isearch-ui",
  "version": "1.0.9",
  "description": "UI for Inspirational Search",
  "main": "src/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run lint && npm run generate-graphql-schema && npm run coverage",
    "dev:build": "webpack --config ./webpack.config.js",
    "dev:serve": "webpack-dev-server --config ./webpack.config.js",
    "lint": "semistandard",
    "lib:test": "mocha ./lib/**/*.test.js --compilers js:babel-register",
    "src:test": "npm run generate-graphql-schema && mocha ./test/constants/queries.test.js --compilers js:babel-register",
    "nocov": "npm run lib:test && npm run src:test",
    "coverage": "./node_modules/.bin/babel-node ./node_modules/.bin/babel-istanbul cover ./node_modules/.bin/_mocha ./test/**/*.test.js ./lib/**/*.test.js --report lcov -- -R spec",
    "clean": "rm -rf public && mkdir -p public/",
    "build": "npm run clean && ./node_modules/.bin/webpack --config ./webpack.production.config.js",
    "deploy": "gulp deploy",
    "generate-graphql-schema": "babel-node ./test/constants/helpers/update-schema.js > ./test/constants/helpers/schema.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/numo-labs/isearch-ui.git"
  },
  "keywords": [
    "React",
    "UI",
    "Search",
    "Grid"
  ],
  "author": "Nikki Ravi & Jack Carlisle & Jimmy Ruts",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/numo-labs/isearch-ui/issues"
  },
  "homepage": "https://github.com/numo-labs/isearch-ui#readme",
  "devDependencies": {
    "aws-sdk": "^2.2.41",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0",
    "babel-istanbul": "^0.6.0",
    "babel-loader": "^6.2.3",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.6.0",
    "bl": "^1.1.2",
    "chai": "^3.5.0",
    "css-loader": "^0.19.0",
    "enzyme": "^2.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "font-awesome-webpack": "0.0.4",
    "gulp": "^3.9.1",
    "html-webpack-plugin": "^2.9.0",
    "imports-loader": "^0.6.5",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "mocha-jsdom": "^1.1.0",
    "react-addons-test-utils": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1",
    "semistandard": "^7.0.5",
    "simple-mock": "^0.6.0",
    "sinon": "^1.17.3",
    "style-loader": "^0.12.4",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "file-loader": "^0.8.5",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.11.1",
    "lodash.shuffle": "^4.0.0",
    "moment": "^2.12.0",
    "react": "^0.14.7",
    "react-bootstrap": "^0.28.5",
    "react-bootstrap-date-picker": "^2.0.0",
    "react-bootstrap-datetimepicker": "0.0.22",
    "react-dom": "^0.14.7",
    "react-masonry-component": "^4.0.0",
    "react-redux": "^4.4.0",
    "react-select": "^0.9.1",
    "react-simple-datepicker": "^0.1.4",
    "react-slick": "^0.12.0",
    "react-typeahead": "^1.1.9",
    "react-visibility-sensor": "^3.0.0",
    "request": "^2.65.0",
    "redux": "^3.3.1"
  },
  "semistandard": {
    "globals": [
      "describe",
      "it",
      "dataLayer",
      "afterEach"
    ],
    "parser": "babel-eslint"
  }
}
