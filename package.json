{
  "name": "isearch-ui",
  "version": "1.0.22",
  "description": "UI for Inspirational Search",
  "main": "src/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run lint && npm run generate-graphql-schema && npm run coverage",
    "dev:build": "webpack -d --config ./webpack.config.js",
    "dev:serve": "webpack-dev-server -d --config ./webpack.config.js  --host 0.0.0.0 --port 8080",
    "lint": "semistandard",
    "lib:test": "mocha ./lib/**/*.test.js --compilers js:babel-register",
    "src:test": "npm run generate-graphql-schema && mocha ./test/**/*.test.js --compilers js:babel-register",
    "nocov": "npm run lib:test && npm run src:test",
    "coverage": "./node_modules/.bin/babel-node ./node_modules/.bin/babel-istanbul cover ./node_modules/.bin/_mocha ./test/**/*.test.js ./lib/**/*.test.js --report lcov -- -R spec",
    "clean": "rm -rf public && mkdir -p public/",
    "ci:build": "npm run clean && ./node_modules/.bin/webpack --optimize-dedupe -p --config ./webpack.ci.config.js",
    "prod:build": "npm run clean && ./node_modules/.bin/webpack --optimize-dedupe -p --config ./webpack.production.config.js",
    "stats": "npm run clean && ./node_modules/.bin/webpack --optimize-dedupe -p --config ./webpack.production.config.js --profile --json > public/stats.json",
    "ci:deploy": "gulp ci:deploy",
    "prod:deploy": "gulp prod:deploy",
    "generate-graphql-schema": "babel-node ./test/constants/helpers/update-schema.js > ./test/constants/helpers/schema.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/numo-labs/isearch-ui.git"
  },
  "keywords": [
    "React",
    "UI",
    "Search",
    "Grid"
  ],
  "author": "Nikki Ravi & Jack Carlisle & Jimmy Ruts",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/numo-labs/isearch-ui/issues"
  },
  "homepage": "https://github.com/numo-labs/isearch-ui#readme",
  "devDependencies": {
    "aws-sdk": "^2.2.41",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0",
    "babel-istanbul": "^0.6.0",
    "babel-loader": "^6.2.3",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.6.0",
    "bl": "^1.1.2",
    "chai": "^3.5.0",
    "con.figure": "1.0.0",
    "css-loader": "^0.23.1",
    "engine.io-client": "^1.6.9",
    "enzyme": "^2.2.0",
    "expect": "^1.19.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "graphql": "^0.4.14",
    "gulp": "^3.9.1",
    "html-webpack-plugin": "^2.9.0",
    "imports-loader": "^0.6.5",
    "jsdom": "^8.1.0",
    "mime-types": "^2.1.11",
    "mocha": "^2.4.5",
    "mocha-jsdom": "^1.1.0",
    "mock-require": "^1.2.1",
    "node-zopfli": "^1.4.0",
    "pre-commit": "^1.1.3",
    "react-addons-test-utils": "^0.14.8",
    "react-autosuggest": "^3.7.4",
    "react-dom": "^0.14.8",
    "react-hot-loader": "^1.3.0",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1",
    "request": "^2.65.0",
    "semistandard": "^7.0.5",
    "simple-mock": "^0.6.0",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.12.4",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "file-loader": "^0.8.5",
    "history": "^2.1.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash.capitalize": "^4.1.1",
    "lodash.union": "^4.4.0",
    "lodash.uniqby": "^4.5.0",
    "moment": "^2.12.0",
    "querystring": "^0.2.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-google-maps": "^4.11.0",
    "react-masonry-component": "^4.0.0",
    "react-redux": "^4.4.0",
    "react-router": "^2.3.0",
    "react-router-redux": "^4.0.4",
    "react-router-scroll": "^0.2.0",
    "react-scroll": "^1.0.15",
    "react-slick": "^0.12.0",
    "react-visibility-sensor": "^3.0.0",
    "redux": "^3.5.2",
    "shuffle-array": "^1.0.0",
    "striptags": "^2.1.1",
    "uuid": "^2.0.2",
    "whatwg-fetch": "^1.0.0"
  },
  "pre-commit": [
    "test"
  ],
  "semistandard": {
    "ignore": [
      "/src/services/primus.js",
      "/src/utils/debugger.js",
      "/src/utils/justice.all.js"
    ],
    "globals": [
      "fetch",
      "describe",
      "it",
      "dataLayer",
      "after",
      "afterEach",
      "before",
      "beforeEach",
      "primus",
      "window",
      "localStorage",
      "$",
      "impressionsTimestamp"
    ],
    "parser": "babel-eslint"
  }
}
